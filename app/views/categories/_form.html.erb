<%= link_to 'Voltar', categories_path %>

<%= form_with model: category do |f| %>
    <div>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div>
      <%= f.label :code %>
      <%= f.text_field :code %>
    </div>
    <div>
    <%= f.submit 'Salvar' %>
    </div>
  <% end %>